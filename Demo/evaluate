max=$1
file_nums=$2
name=elapsed_time.txt

touch $name
echo -e "\n// demo_init" >> $name

for ((i=0; i < $max; i++)); do
    make clean && make && ./init $file_nums && ./demo -i $file_nums -t 
done

echo -e "\n// demo_check" >> $name

for ((i=0; i < $max; i++)); do
    ./demo -c $file_nums -t 
done

make clean